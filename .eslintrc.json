{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-restricted-globals": "off",
        "no-restricted-syntax": "off",
        "no-trailing-spaces": "off",
        "no-underscore-dangle": "off",
        "no-param-reassign": "off",
        "no-case-declarations": "off",
        "no-console": "off",
        "radix": "off",
        "curly": "off",
        "global-require": "off"
    }
}
